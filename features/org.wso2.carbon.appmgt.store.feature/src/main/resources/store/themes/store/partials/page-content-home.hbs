{{!-- page content - home page apps --}}
{{#each assetTypes}}

    {{#if assets}}

        <div class="row page-title">
                <div class="container-fluid">
                    <div class="title">{{plural}}</div><!--title-->
                    <div class="float-right"><a class="more" href="{{url seeMoreUrl}}">See More</a></div>
                    <hr>
                </div>
        </div>
            {{#ifCond type "==" "mobileapp"}}
                {{> page-content-home/mobileapp}}
            {{else}}
                {{#if ../../../subscriptionOn}}
                    {{> page-content-home/subscription-on}}
                {{else}}
                    {{> page-content-home/subscription-off}}
                {{/if}}
            {{/ifCond}}


    {{/if}}
{{/each}}

