<div>
    {{#unless isEnterpriseSubscriptionEnabled}}
        {{#unless isSelfSubscriptionEnabled}}
            <input type="hidden" value="true" id="subscription-off" name="subscriptionOff">
        {{/unless}}
    {{/unless}}

	{{#if isSubscribed}}
		<input type="hidden" value="true" id="subscribed" name="subscribed">
	{{else}}
		<input type="hidden" value="false" id="subscribed" name="subscribed">
	{{/if}}

    {{#if skipGateway}}

        <legend>URLs:</legend>
        <p><b>Web App Endpoint</b></p>
        <ul>
        <li><a class='icon-globe' id="gatewayURL" href="{{asset.attributes.overview_webAppUrl}}" target="_blank" >&nbsp;{{asset.attributes.overview_webAppUrl}}</a></li>
        </ul>
        <input type="hidden" id="skipGateway" value="true">
    {{else}}
        {{#with apiData.serverURL}}
    		<legend>URLs:</legend>
	    	<p><b>{{description}}</b></p>
	    	<ul>
			<li><a class='icon-globe' id="gatewayURL" href="{{productionURL}}" target="_blank" >&nbsp;{{productionURL}}</a></li>
	    	</ul>
            <input type="hidden" id="skipGateway" value="false">
	    {{/with}}
    {{/if}}

	<div class="modal fade" id="messageModal2" style="display: none"></div>
	<div id="confirmation-data2" style="display: none;">
		<div class="modal-header">
			<button class="close" data-dismiss="modal">&times;</button>
			<h3 class="modal-title">Modal header</h3>
		</div>
		<div class="modal-body">
			<p>One fine bodyâ€¦</p>
		</div>
		<div class="modal-footer">
			<a href="#" class="btn btn-other" data-dismiss="modal">Close</a>
		</div>
	</div>
</div>

