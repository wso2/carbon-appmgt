<%
include("/jagg/jagg.jag");

(function () {
    var log=new Log();
    var action = request.getParameter("action");
    if (action === "sessionCheck") {
         if (log.isDebugEnabled()) {
            log.debug("session check for" + jagg.getUser());
         }
         if(jagg.getUser() == null){
            print({
                error:true,
                action:'timeout'
            });
        }
    } else {
        print({
            error:true,
            action:msg.error.invalidAction(action)
        });
    }
}());
%>