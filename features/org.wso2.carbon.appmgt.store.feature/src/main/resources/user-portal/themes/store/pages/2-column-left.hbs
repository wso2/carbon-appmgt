<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{{title}}</title>

    <link rel="shortcut icon" href="{{customThemeUrl "/libs/theme-wso2_1.0/images/favicon.png"}}" />
    <!-- Bootstrap CSS -->
    <link href="{{customThemeUrl "/libs/bootstrap_3.3.5/css/bootstrap.min.css"}}" rel="stylesheet" type="text/css" />
    <!-- Font WSO2 CSS -->
    <link href="{{customThemeUrl "/libs/font-wso2_1.0.0/css/font-wso2.min.css"}}" rel="stylesheet" type="text/css" />
    <!-- Theme WSO2 CSS -->
    <link href="{{customThemeUrl "/libs/theme-wso2_1.0/css/theme-wso2.css"}}" rel="stylesheet" type="text/css" />
    <!-- AppM CSS -->
    <link href="{{customThemeUrl "/css/appm-main-styles.css"}}" rel="stylesheet" type="text/css" />
    <link href="{{customThemeUrl "/css/appm-left-column-styles.css"}}" rel="stylesheet" type="text/css" />
    <!--Tour guide CSS-->
    <link href="{{url "/themes/store/libs/bootstrap-tour-0.10.1/bootstrap-tour.css"}}" rel="stylesheet" type="text/css" />
    <!-- Date-range-picker CSS -->
    <link href="{{url "/themes/store/libs/date-range-picker_2.1.19/css/daterangepicker.css"}}" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="{{customThemeUrl "/libs/html5shiv_3.7.2/html5shiv.min.js"}}"></script>
    <script src="{{customThemeUrl "/libs/respond_1.4.2/respond.min.js"}}"></script>
    <![endif]-->

    {{#if googleAnalytics.enabled}}
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function()
            { (i[r].q=i[r].q||[]).push(arguments)}

            ,i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

            ga('create', '{{googleAnalytics.trackerId}}', 'auto');
            ga('send', 'pageview');

            </script>

        <!-- Google Tag Manager -->
        <noscript><iframe src="//www.googletagmanager.com/ns.html?id=GTM-WJKF4T"
                          height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
        <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push(
            {'gtm.start': new Date().getTime(),event:'gtm.js'}

            );var f=d.getElementsByTagName(s)[0],
            j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
            '//www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
            })(window,document,'script','dataLayer','GTM-WJKF4T');</script>
        <!-- End Google Tag Manager -->
    {{/if}}

    <!--other CSS -->
    {{css}}
</head>

<body>
<!-- header -->
{{include header}}

<!-- page content wrapper -->
<div class="page-content-wrapper">
    <div class="media">
        <!-- media left -->
        <div class="media-left sidebar-wrapper visible-side-pane" id="left-sidebar" data-offset-top="80"
             data-side="left" data-width="200">
            {{include leftColumn}}
        </div>

        <!-- media body -->
        <div class="media-body">
            <!-- page -->
            <div class="container-fluid body-wrapper">
                <div id="page-header" class="page-header">
                    <div class="row">
                        <div class="col-md-12">
                            {{include pageHeader}}

                            {{include search}}
                        </div>
                    </div>

                </div>

                <div class="page-content">
                    {{include pageContent}}
                </div>
            </div>
        </div>
    </div>
</div>

<!-- footer -->
{{>footer}}

<!-- Jquery/Jquery UI JS -->
<script src="{{customThemeUrl "/libs/jquery_1.11.0/jquery-1.11.3.min.js"}}"></script>
<!-- Bootstrap JS -->

<script src="{{url "/themes/store/libs/bootstrap_3.3.5/js/bootstrap.min.js"}}"></script>
<!-- Date-range-picker UI JS -->
<script src="{{url "/themes/store/libs/date-range-picker_2.1.19/js/moment.min.js"}}"></script>
<script src="{{url "/themes/store/libs/date-range-picker_2.1.19/js/daterangepicker.js"}}"></script>
<!-- Theme WSO2 JS -->
<script src="{{customThemeUrl "/libs/theme-wso2_1.0/js/theme-wso2.js"}}"></script>
<!-- noty JS -->
<script src="{{customThemeUrl "/js/noty/jquery.noty.js"}}"></script>
<script src="{{customThemeUrl "/js/noty/layouts/center.js"}}"></script>
<script src="{{customThemeUrl "/js/noty/themes/default.js"}}"></script>
<!-- history -->
<script src="{{customThemeUrl "/js/jquery.history.js"}}"></script>
<script src="{{customThemeUrl "/js/history-fix.js"}}"></script>
<!--Tour guide JS-->
<script src="{{customThemeUrl "/js/../libs/bootstrap-tour-0.10.1/bootstrap-tour.min.js"}}"></script>
<!--other JS -->
{{js}}
<!-- handlebars JS  -->
<script src="{{customThemeUrl "/js/handlebars/handlebars.js"}}"></script>
<!-- Caramel client-side JS -->
<script src="{{customThemeUrl "/js/caramel/caramel-client.js"}}"></script>
<script src="{{customThemeUrl "/js/caramel/caramel.handlebars.client.js"}}"></script>
<script src="{{customThemeUrl "/js/caramel/async.min.js"}}"></script>
<!-- Event publisher JS -->
<script src="{{customThemeUrl "/js/writeToPublisher.js"}}"></script>
<!-- AppM theme related JS -->
<script src="{{customThemeUrl "/js/appm/appm-styles.js"}}"></script>
<!-- AppM - Search JS -->
<!-- AppM -user login/register JS -->
<script src="{{customThemeUrl "/js/appm/login.js"}}"></script>
<script src="{{customThemeUrl "/js/appm/tenantedURL.js"}}"></script>
<script src="{{customThemeUrl "/js/jquery.validate.min.js"}}"></script>
</body>
</html>
